paket  sesi11 ;

 kelas  publik ThrowExample {
    
     demo kekosongan  statis (){
        NullPointerException  t ;
        t = new  NullPointerException ( "Coba Throw" );
        melempar  t ;
        // Baris ini tidak lagi dikerjakan;
        //System.out.println("Ini tidak lagi dicetak");
    }
    public  static  void  main ( String [] args ) {
        coba {
            demo ();
            Sistem . keluar . println ( "Selesai" );
        }
        tangkap ( NullPointerException  e ){
            Sistem . keluar . println ( "Ada pesan error: " + e );
        }
    }
}
