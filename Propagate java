paket  sesi11 ;

 kelas  publik Sebarkan {
    public  static  void  main ( String [] args ){
        coba {
            Sistem . keluar . println ( balik ( "" ));
        }
        tangkap ( Pengecualian  e ){
            Sistem . keluar . println ( "String kosong" );
        }
        akhirnya {
            Sistem . keluar . println ( "Selesai" );
        }
    }

    public  static  String  terbalik ( String  s ) melempar  Pengecualian {
        if ( s .panjang ( ) == 0 ){
            lempar  Pengecualian baru  ();
        }
        String  kebalikanStr = "" ;
        untuk ( int  i = s .panjang ( ) - 1 ; i >= 0 ; -- i ){
            reverseStr += s . charAt ( i );
        }

        kembalikan  reverseStr ;
    }
}
